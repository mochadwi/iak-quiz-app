<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.co.iak.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".question.Question01Activity"></activity>
        <activity android:name=".question.Question02Activity"></activity>
        <activity android:name=".question.Question03Activity"></activity>
        <activity android:name=".question.Question04Activity"></activity>
        <activity android:name=".question.Question05Activity"></activity>
        <activity android:name=".question.ResultActivity"></activity>
        <activity android:name=".question.AnswerActivity"></activity>
    </application>

</manifest>